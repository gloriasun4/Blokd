{
    "name": "Blokd",
    "version": "1.0",
    "manifest_version": 2,
    "icons":{
        "128": "blokdlogo-01.png"
    },

    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],

    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Blokd"
    }    
  }